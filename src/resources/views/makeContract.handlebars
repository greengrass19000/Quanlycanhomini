
<div class = "mt-4">
  <div class = "row">
    <div class="container">
        {{#each contractData}}
            <form action = "/host/contractAdded?id={{../id}}&type={{../type}}" method = "post">
                <div class="form-group">
                    <label for="exampleFormControlInput1">Mã phòng</label>
                    <input class="form-control" name = "roomID" id = "roomID" value = {{this.id}} readonly>
                </div>
                <div class="form-group">
                    <label for="exampleFormControlInput1">Mã chủ nhà</label>
                    <input class="form-control" name = "hostID" id = "hostID" value = {{../id}} readonly>
                </div>
                <div class="form-group">
                    <label for="exampleFormControlInput1">Mã khách hàng</label>
                    <input class="form-control" name = "renterID" id="renterID">
                </div>
                <div class="form-group">
                    <label for="exampleFormControlInput1">Giá tiền</label>
                    <input class="form-control" name ="deposit" id="deposit">
                </div>
                <div class="col-md-5">
                    <label for="birthdate" class="form-label">Ngày bắt đầu thuê</label>
                    <input type="date" class="form-control" id="startTime" name = "startTime">
                </div>
                <div class="col-md-5">
                    <label for="birthdate" class="form-label">Ngày kết thúc</label>
                    <input type="date" class="form-control" id="endTime" name = "endTime">
                </div>
                 <div class="form-group">
                    <label for="exampleFormControlInput1">Mô tả</label>
                    <input class="form-control" name ="description" id="description">
                </div>
                <button class="btn btn-primary btn" type="submit">Thêm</button>
            </form>
        {{/each}}
    </div>
  </div>
</div>
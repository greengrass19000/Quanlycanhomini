<div class = "mt-4">
  <div class = "row">
    <div class="container">
        {{#each buildingData}}
            <form action = "/host/buildingAdded?id={{../id}}&type={{../type}}" method = "post">
                <div class="form-group">
                    <label for="exampleFormControlInput1">Mã chủ nhà</label>
                    <input class="form-control" name = "hostID" id="hostID" value={{../id}} readonly>
                </div>
                <div class="form-group">
                    <label for="exampleFormControlInput1">Quận</label>
                    <input class="form-control" name = "district" id="district">
                </div>
                <div class="form-group">
                    <label for="exampleFormControlInput1">Phường</label>
                    <input class="form-control" name = "ward" id="ward">
                </div>
                <div class="form-group">
                    <label for="exampleFormControlInput1">Đường</label>
                    <input class="form-control" name ="street" id="street">
                <div class="form-group">
                    <label for="exampleFormControlInput1">Ảnh</label>
                    <input class="form-control" name = "image" id="image">
                <hr class="my-4">

                <button class="btn btn-primary btn" type="submit">Thêm</button>
            </form>
        {{/each}}
    </div>
  </div>
</div>